<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <servlet>
    <servlet-name>mainServlet</servlet-name>
    <servlet-class>com.example.server.ServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>mainServlet</servlet-name>
    <url-pattern>/MyGwtApp/main</url-pattern>
  </servlet-mapping>


  <welcome-file-list>
    <welcome-file>MyGwtApp.html</welcome-file>
  </welcome-file-list>

</web-app>
